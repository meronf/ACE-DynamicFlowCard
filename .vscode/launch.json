{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Hosted workbench",
      "type": "msedge",
      "request": "launch",
      "url": "https://tenantName.sharepoint.com/_layouts/workbench.aspx?debug=true&noredir=true&debugManifestsFile=http://localhost:4321/temp/manifests.js",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///.././src/*": "${webRoot}/src/*",
        "webpack:///../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../../src/*": "${webRoot}/src/*"
      },
      "runtimeArgs": [
        "--remote-debugging-port=9222",
        "--profile-directory=Profile 7",
        "--user-data-dir=C:\\Users\\%USERNAME%\\AppData\\Local\\Microsoft\\Edge\\User Data",
        "--disable-web-security",
        "--disable-features=VizDisplayCompositor"
      ]
    }
  ]
}
